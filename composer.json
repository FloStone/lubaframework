{
    "name": "flo5581/lubaframework",
    "description": "Application Package for the Luba Framework",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Flo Stone"
        }
    ],
    "require": {
        "symfony/var-dumper": "3.0.*",
        "Flo5581/mysql": "dev-master"
    },
    "autoload": {
