{
    "name": "flostone/lubaframework",
    "description": "Application Package for the Luba Framework",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Flo Stone"
        }
    ],
    "require": {
        "symfony/var-dumper": "dev-master",
        "flostone/mysql": "dev-master",
        "Lubaaddons/Exceptionhandler": "dev-master"
    },
    "autoload": {
        "files": [
            "globals/functions.php"
        ],
        "psr-4": {
            "Luba\\Framework\\": "luba/",
            "Luba\\Exceptions\\": "exceptions/",
            "Luba\\Interfaces\\": "interfaces/",
            "Luba\\Traits\\": "traits/",
            "Luba\\Form\\": "form/",
            "Luba\\Commands\\": "commands/"
        },
        "classmap": [
            "aliases"
        ]
    }
}

