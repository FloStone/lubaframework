{
    "name": "flostone/lubaframework",
    "description": "Application Package for the Luba Framework",
    "version": "1.3.1",
    "authors": [
        {
            "name": "Flo Stone"
        }
    ],
    "require": {
        "symfony/var-dumper": "3.0.*",
        "flostone/mysql": "1.3.*",
        "Lubaaddons/Exceptionhandler": "1.0.*"
    },
    "autoload": {
        "files": [
            "globals/functions.php"
        ],
        "psr-4": {
            "Luba\\Framework\\": "luba/",
            "Luba\\Exceptions\\": "exceptions/",
            "Luba\\Interfaces\\": "interfaces/",
            "Luba\\Traits\\": "traits/",
            "Luba\\Form\\": "form/",
            "Luba\\Commands\\": "commands/"
        },
        "classmap": [
            "aliases"
        ]
    }
}

